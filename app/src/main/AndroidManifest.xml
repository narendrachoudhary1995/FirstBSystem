<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.narendra.travelersbridge">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CreateAccount"  android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.SearchFlight"
            android:label="@string/title_activity_search_flight1"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".Activity.SearchCity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".Activity.FlightSearchResult"
            android:screenOrientation="portrait"
            />
        <activity android:name=".Activity.PassengerDetails"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.ViewProfile"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.Transaction_History"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.SendQuery"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.RoundTripResult"
            android:screenOrientation="portrait"
            ></activity>
    </application>

</manifest>